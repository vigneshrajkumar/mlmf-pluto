<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Home">

    <VBox alignment="TOP_CENTER" prefHeight="110.0" prefWidth="284.0">
        <Label text="My Life - My Future">
         <font>
            <Font size="23.0" />
         </font></Label>
        <Label text="The Ministo Foundation">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
        <HBox prefHeight="47.0" prefWidth="286.0">
        <Button styleClass=".f" text="Validate CSV" fx:id="vValidateCSV" onAction="#validateCSV">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
        <Button styleClass=".f" text="Generate Reports" fx:id="vGenerateReports" onAction="#generateReports">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></Button>
        </HBox>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>

</AnchorPane>
